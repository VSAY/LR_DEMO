<script type="text/javascript">
	function getUrlCustomVars()
	{
	    var vars = [], hash;
	    var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
	    for(var i = 0; i < hashes.length; i++)
	    {
	        hash = hashes[i].split('=');
	        vars.push(hash[0]);
	        vars[hash[0]] = hash[1];
	    }
	    return vars;
	}
	if(typeof adobeDataLayer != "undefined"){
		if(typeof customPageType != "undefined"){
			adobeDataLayer['pageType'] = customPageType;
		}
		if(typeof customSiteSection != "undefined"){
			adobeDataLayer['siteSection'] = customSiteSection;
		}
		if(typeof customSiteSubSection != "undefined"){
			adobeDataLayer['siteSubSection'] = customSiteSubSection;
		}
		if(typeof customPageName != "undefined"){
			adobeDataLayer['pageName'] = customPageName;
		}
		var customSearchTerm = getUrlCustomVars()["_3_keywords"];
		if(typeof customSearchTerm != "undefined"){
			adobeDataLayer['searchTerm'] = customSearchTerm;
			adobeDataLayer['searchType'] = "ALL";
			adobeDataLayer['searchResults'] = "NA";
		}
	}
</script>